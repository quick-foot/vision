{
    "name": "vision",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "build:watch": "ng build --watch",
        "test": "ng test",
        "coverage": "ng test --no-watch --code-coverage",
        "lint": "ng lint --fix=true --format=stylish",
        "e2e": "ng e2e",
        "ts-node": "ts-node -P scripts/tsconfig.json -r tsconfig-paths/register",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "app-version": "npm run ts-node ./scripts/version.ts && git add src/app/core/version.ts",
        "version": "npm run changelog && npm run app-version",
        "postversion": "git push --follow-tags"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "7.2.15",
        "@angular/common": "7.2.15",
        "@angular/compiler": "7.2.15",
        "@angular/core": "7.2.15",
        "@angular/flex-layout": "7.0.0-beta.24",
        "@angular/forms": "7.2.15",
        "@angular/platform-browser": "7.2.15",
        "@angular/platform-browser-dynamic": "7.2.15",
        "@angular/router": "7.2.15",
        "core-js": "2.6.5",
        "rxjs": "6.5.2",
        "tslib": "1.9.3",
        "zone.js": "0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.13.9",
        "@angular/cli": "7.3.9",
        "@angular/compiler-cli": "7.2.15",
        "@angular/language-service": "7.2.15",
        "@quick-foot/tslint-config": "1.1.1",
        "@types/jasmine": "3.3.12",
        "@types/jasminewd2": "2.0.6",
        "@types/node": "12.0.1",
        "@types/webpack-env": "1.13.9",
        "codelyzer": "4.5.0",
        "colors": "1.3.3",
        "conventional-changelog": "3.1.8",
        "jasmine-core": "3.4.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "4.1.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.5",
        "karma-jasmine": "2.0.1",
        "karma-jasmine-html-reporter": "1.4.2",
        "protractor": "5.4.2",
        "ts-node": "8.1.0",
        "tsconfig-paths": "3.8.0",
        "tslint": "5.16.0",
        "typescript": "3.2.2"
    }
}
